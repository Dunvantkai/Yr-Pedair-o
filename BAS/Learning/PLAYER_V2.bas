   10 MODE 2
   20 X=100
   30 Y=100
   35 Z=0
   40 PRINT "Press 'Q' to quit"
   50 REM CALL TO LOOP
   60 K=INKEY(0)
   70 REM IF K <> -1 THEN PRINT "KEY PRESSED: " ; CHR$(K)
   75 IF K=81 THEN END :REM Q
   80 IF K=87 THEN GOTO 130 :REM W
   90 IF K=83 THEN GOTO 140 :REM S
  100 IF K=68 THEN GOTO 150 :REM D
  110 IF K=65 THEN GOTO 160 :REM A
  115 IF Z=0 GOTO 50 ELSE PRINT "Z WAS CHANEDS" : END
  130 Y=Y+20 : GOTO 170 :REM W
  140 Y=Y-20 : GOTO 170 :REM S
  150 X=X+20 : GOTO 170 :REM D
  160 X=X-20 : GOTO 170 :REM A
  170 MOVE X,Y
  175 CLS
  180 DRAW X,Y+100
  190 DRAW X+100,Y+100
  200 DRAW X+100,Y
  210 DRAW X,Y
  230 GOTO 50


