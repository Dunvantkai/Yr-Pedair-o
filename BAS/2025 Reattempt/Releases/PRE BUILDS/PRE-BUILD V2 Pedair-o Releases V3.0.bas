  202 DEF PROC_DROP
  203 FOR SX=1 TO 11 : FOR SY=0 TO 8 :SCREEN$(SX,SY)=SCREEN$(SX,SY)+BL$ : NEXT SY : NEXT SX
  204 PROC_CHECKREMOVELINE
