  206 DEF PROC_LEFT
  207 FOR SX=0 TO 3:FOR SY=0 TO 3:TMP$=SETHB$(R,SX,SY):IF TMP$=BL$:IF TMP$=SCREEN$(SX+X,SY+Y) THEN COL=TRUE :REM Collision Check
  208 NEXT SY:NEXT SX
  209 IF COL=FALSE THEN PRINTTAB(X+14,Y+3)NSET$(R):X=X-1:PRINTTAB(X+14,Y+3)SET$(R):ENDPROC:REM Collision Failed
  210 COL=FALSE
  211 ENDPROC
  212 DEF PROC_RIGHT
  213 FOR SX=0 TO 3:FOR SY=0 TO 3:TMP$=SETHB$(R,SX,SY):IF TMP$=BL$:IF TMP$=SCREEN$(SX+X+2,SY+Y) THEN COL=TRUE :REM Collision Check
  214 NEXT SY:NEXT SX
  215 IF COL=FALSE THEN PRINTTAB(X+14,Y+3)NSET$(R):X=X+1:PRINTTAB(X+14,Y+3)SET$(R):ENDPROC:REM Collision Failed
  216 COL=FALSE
  217 ENDPROC
  218 DEF PROC_ROTATE
  219 TR=R+1 : IF TR>3 THEN TR=0
  220 FOR SX=0 TO 3:FOR SY=0 TO 3:TMP$=SETHB$(TR,SX,SY):IF TMP$=BL$:IF TMP$=SCREEN$(SX+X+1,SY+Y) THEN COL=TRUE :REM Collision Check
  221 NEXT SY:NEXT SX
  222 IF COL=FALSE THEN PRINTTAB(X+14,Y+3)NSET$(R):R=TR:PRINTTAB(X+14,Y+3)SET$(R):ENDPROC:REM Collision Failed
  223 COL=FALSE
  224 ENDPROC
  225 DEF PROC_DOWN
  226 FOR SX=0 TO 3:FOR SY=0 TO 3:TMP$=SETHB$(R,SX,SY):IF TMP$=BL$:IF TMP$=SCREEN$(SX+X+1,SY+Y+1) THEN COL=TRUE :REM Collision Check
  227 NEXT SY : NEXT SX
  228 IF COL=FALSE THEN PRINTTAB(X+14,Y+3)NSET$(R):Y=Y+1:PRINTTAB(X+14,Y+3)SET$(R) :REM Collision Failed
  229 IF COL=TRUE THEN SCORE%=SCORE%+5:FOR SX=0 TO 3:FOR SY=0 TO 3:SCREEN$(SX+X+1,SY+Y)=SCREEN$(SX+X+1,SY+Y)+SETHB$(R,SX,SY):NEXT SY:NEXT SX:PROC_NEXTBLOCK:PROC_CHECKREMOVELINE:COL=FALSE :REM Collision Successful
  230 ENDPROC
  231 DEF PROC_CHECKREMOVELINE
  232 COL=TRUE : THISREMOVEDLINE%=0 :REM CHECK FOR LINE IS FULL AND SAVING WHAT LINE IS TO BE REMOVED
  233 FOR YA=17 TO 0 STEP -1
  234 FOR XA=0 TO 10
  235 IF COL=TRUE THEN IF SCREEN$(XA+1,YA)<>BL$ THEN COL=FALSE
  236 NEXT XA
  237 IF COL=TRUE THEN IF THISREMOVEDLINE%<YA THEN THISREMOVEDLINE%=YA
  238 CRLP=TRUE : NEXT YA
  239 IF THISREMOVEDLINE%<>0 THEN SCORE%=SCORE%+35 : PROC_REMOVELINE
  240 ENDPROC
  241 DEF PROC_REMOVELINE
  242 FOR YA=THISREMOVEDLINE% TO 1 STEP -1
  243 FOR XA=0 TO 10
  244 SCREEN$(XA+1,YA)=SCREEN$(XA+1,YA-1)
  245 NEXT XA
