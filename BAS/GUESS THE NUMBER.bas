   10 CLS
   15 GOTO 100
   20 NUM=RND(100)
   30 PRINT "I AM THINKING OF A NUMBER "
   32 PRINT "BETWEEN 1 AND 100"
   34 PRINT
   35 PRINT "WHAT NUMBER AM I THINKING OF ";
   40 INPUT GUESS
   50 IF GUESS > NUM THEN PRINT "LOWER"; : SOUND 1,-15,100,1
   60 IF GUESS < NUM THEN PRINT "HIGHER"; : SOUND 1,-15,140,1
   70 IF GUESS = NUM THEN PRINT "YOU GUESSED IT!": GOTO 80 ELSE 40
   80 S=255
   81 L=0
   85 SOUND 1,-15,S,1
   90 S=S-76
   91 L=L+1
   95 IF L=25 THEN GOTO 100 ELSE GOTO 85
  100 MODE 0
  120 X=RND(1000)
  130 Y=RND(1000)
  140 MOVE 500,500
  150 DRAW X,Y
  160 GOTO 120
