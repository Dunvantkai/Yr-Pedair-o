   10 MODE 7
   20 BL$=CHR$(255) :REM BLOCK
   30 B$=CHR$(8)    :REM CURSOR BACK
   40 F$=CHR$(9)    :REM CURSOR FORWARD
   50 D$=CHR$(10)   :REM CURSOR DOWN
   60 U$=CHR$(11)   :REM CURSOR UP
   64 X=1
   65 Y=1
   70 Z=0
   75 GR$=CHR$(129)
   76 PRI$="PRESS W,A,S,D TO MOVE AND Q TO RESTART"
   80 OB$=BL$+BL$+D$+B$+B$+BL$+BL$
   90 IB$=BL$+D$+B$+BL$+D$+B$+BL$+D$+B$+BL$
  100 SB$=F$+BL$+BL$+D$+B$+B$+B$+BL$+BL$+F$
  110 ZB$=BL$+BL$+D$+B$+B$+F$+BL$+BL$
  120 LB$=BL$+D$+B$+BL$+D$+B$+BL$+BL$
  130 JB$=F$+BL$+D$+B$+BL$+D$+B$+B$+BL$+BL$
  140 TB$=BL$+BL$+BL$+D$+B$+B$+BL$
  141 DIM OBA$(1)
  142 OBA$(0) = BL$+BL$+D$+B$+B$+BL$+BL$
  143 OBA$(1) = BL$+BL$+BL$+BL$
  150 PRINTTAB(0,0)OB$ ; :PRINT " BLOCK O"
  160 PRINTTAB(20,0)TB$ ; :PRINT " BLOCK T"
  170 PRINTTAB(0,5)SB$ ; :PRINT " BLOCK S"
  180 PRINTTAB(20,5)ZB$ ; :PRINT " BLOCK Z"
  190 PRINTTAB(0,10)LB$ ; :PRINT " BLOCK L"
  200 PRINTTAB(20,10)JB$ ; :PRINT " BLOCK J"
  210 PRINTTAB(0,15)IB$ ; :PRINT " BLOCK I"
  215 PRINT
  220 PRINT "PLEASE ENTER A SHAPE ID " ;
  230 INPUT SEB$
  240 IF SEB$="O" THEN SEB$=OB$ : CLS : PRINT GR$PRI$
  250 IF SEB$="T" THEN SEB$=TB$ : CLS : PRINT GR$PRI$
  260 IF SEB$="S" THEN SEB$=SB$ : CLS : PRINT GR$PRI$
  270 IF SEB$="Z" THEN SEB$=ZB$ : CLS : PRINT GR$PRI$
  280 IF SEB$="L" THEN SEB$=LB$ : CLS : PRINT GR$PRI$
  290 IF SEB$="J" THEN SEB$=JB$ : CLS : PRINT GR$PRI$
  300 IF SEB$="I" THEN SEB$=IB$ : CLS : PRINT GR$PRI$
  310 REM CALL TO LOOP
  320 K=INKEY(0)
  330 IF K=87 THEN GOTO 420 :REM W
  340 IF K=83 THEN GOTO 430 :REM S
  350 IF K=68 THEN GOTO 440 :REM D
  360 IF K=65 THEN GOTO 450 :REM A
  365 IF K=81 THEN CLS :X=1 :Y=1 :GOTO 150:REM PRESS Q
  370 IF X>38 THEN X=38
  380 IF X<0 THEN X=0
  390 IF Y>25 THEN Y=25
  400 IF Y<0 THEN Y=0
  410 IF Z=0 GOTO 310 ELSE PRINT "Z WAS CHANEDS" : END
  420 Y=Y-1 : GOTO 455 :REM W
  430 Y=Y+1 : GOTO 455 :REM S
  440 X=X+1 : GOTO 455 :REM D
  450 X=X-1 : GOTO 455 :REM A
  455 FOR I% = 0 TO 1
  460   CLS
  470   PRINT TAB(X,Y)GR$OBA$(I)
  480   GOTO 310

